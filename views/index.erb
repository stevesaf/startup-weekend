<wrapper>
<div class="container" id="index_header">

  <div class="row">
    <div class="col-md-3">
      <!-- <img class="cd" src="https://media.giphy.com/media/s8crIPpjNHtmw/giphy.gif"/> -->
    </div>
    <div class="col-md-6">
      <h1>Schmartup Wknd</h1>
    </div>
    <div class="col-md-3">
      <!-- <img class="cd" src="https://media.giphy.com/media/s8crIPpjNHtmw/giphy.gif"/> -->
    </div>
  </div>

</div>

<div class="container" id="index_body">
  <form action="/teams" method="post">
    <label for="name">Enter team name here</label><br>
    <input id="name" name="name" type="text"><br>
    <button type="submit">Add</button>
  </form>
  <div id="title_list">
    <ul>
      <% @list.each() do |team| %>
      <li><a href="/teams/<%= team.id() %>"><%= team.name() %></a></li>
      <% end %>
    </ul>
  </div>


</div>
</wrapper>
